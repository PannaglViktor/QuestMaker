<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State when the button is checked (full star) -->
    <item android:drawable="@drawable/ic_star_filled" android:state_checked="true"/>
    <!-- Default state, when the button is not checked (empty star) -->
    <item android:drawable="@drawable/ic_star_border" android:state_checked="false"/>
</selector>
